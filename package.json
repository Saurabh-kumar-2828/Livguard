{
    "private": true,
    "sideEffects": false,
    "scripts": {
        "build": "run-s \"build:*\"",
        "build:css": "npm run css:build -- --env production",
        "build:remix": "remix build",
        "dev": "run-p \"dev:*\"",
        "dev:css": "npm run css:build -- --watch",
        "dev:remix": "remix dev",
        "start": "remix-serve build",
        "prod": "run-s build start",
        "css:build": "postcss app/styles/tailwind.css --output build/tailwind.css"
    },
    "dependencies": {
        "@remix-run/node": "^1.12.0",
        "@remix-run/react": "^1.12.0",
        "@remix-run/serve": "^1.12.0",
        "isbot": "^3.6.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@remix-run/dev": "^1.12.0",
        "@remix-run/eslint-config": "^1.12.0",
        "@types/react": "^18.0.25",
        "@types/react-dom": "^18.0.8",
        "eslint": "^8.27.0",
        "typescript": "^4.8.4"
    },
    "engines": {
        "node": ">=14"
    }
}
